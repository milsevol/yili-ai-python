{
  "aggs": {
    "group_by_customer": {
      "terms": {
        "field": "customer_id",  // 按客户ID分组（去重）
        "size": 200,  // 最多返回200个客户
        "order": {
          "max_knn_score": "desc"  // 客户级排序：按组内最高kNN得分降序
        }
      },
      "aggs": {
        "top3_sessions": {
          "top_hits": {
            "size": 3,  // 每个客户取前3条相似会话
            "sort": [{"_score": {"order": "desc"}}],  // 会话排序：按kNN得分降序
            "_source": [
              "conversation_id",
              "conversation_time",
              "summary",
              "customer_name",
              "advisor_name"
            ]
          }
        },
        "max_knn_score": {
          "max": {"script": "_score"}  // 计算每个客户的最高kNN得分（用于客户排序）
        }
      }
    }
  },
  "size": 0,  // 不返回原始文档，仅保留聚合结果
  "query": {
    "bool": {
      "filter": [
        // 保留业务过滤条件（与原逻辑一致）
        {
          "range": {
            "conversation_time": {
              "gte": "2024-01-01T00:00:00",
              "lte": "2024-12-31T23:59:59",
              "format": "yyyy-MM-dd'T'HH:mm:ss"
            }
          }
        },
        {
          "terms": {
            "mentioned_products": ["产品A", "产品B"]
          }
        }
      ],
      "must": [
        // 核心：用kNN查询替代script_score，实现向量相似度检索
        {
          "knn": {
            "content_vector": {  // 匹配索引中的dense_vector字段（维度1024）
              "vector": [],  // 填入1024维查询向量（如[0.1, 0.2, ..., 1.0]）
              "k": 1000,  // 先获取top1000相似文档（后续聚合会按客户分组）
              "similarity": "cosine"  // 相似度算法（可选cosine/l2/dot_product）
            }
          }
        }
      ]
    }
  }
}